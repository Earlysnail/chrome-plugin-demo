{
	"manifest_version": 2,
	"name": "chrome-plugin-demo",
	"version": "1.0.0",
	"description": "抄送提醒",
	"icons":
	{
		"16": "img/sds.png",
		"48": "img/sds.png",
		"128": "img/sds.png"
	},
	"page_action":
	{
		"default_icon": "img/sds.png",
		"default_title": "我是pageAction",
		"default_popup": "popup.html"
	},
	"permissions":
	[
		"contextMenus",
        "notifications",
		"declarativeContent",
		"webNavigation"
	],
	"background":
	{
		"scripts": ["js/background.js"]
	}
}